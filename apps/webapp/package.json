{
    "name": "webapp",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "packageManager": "pnpm@10.22.0",
    "scripts": {
        "dev": "vite",
        "online": "vite --host",
        "preview": "vite preview",
        "test": "npx vitest --config=vitest.node.config.ts --no-cache",
        "test:w": "npx vitest --config=vitest.node.config.ts --watch",
        "test:v8": "npx vite",
        "lint": "eslint . -f visualstudio --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
        "build": "tsc -b && vite build",
        "pnpm:u": "pnpm update",
        "pnpm:ou": "pnpm outdated",
        "ncu:l": "ncu",
        "ncu:u": "ncu -u",
        "ncu:ud": "ncu -u --dev",
        "nvm:la": "nvm list available",
        "nvm:il": "nvm install lts",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "tw:i": "npx @tailwindcss/cli init",
        "tw:w": "npx @tailwindcss/cli -i ./src/app.css -o ./src/app-full.css --watch",
        "postcss": "ts-node postcss.ts",
        "az-cr:cl": "powershell -ExecutionPolicy Bypass -File az_cr_cleanup.ps1"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.933.0",
        "@aws-sdk/s3-request-presigner": "^3.933.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@iconify/react": "^6.0.2",
        "@mui/material": "^7.3.5",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lodash-es": "^4.17.21",
        "mobx": "^6.15.0",
        "mobx-react-lite": "^4.1.1",
        "p-limit": "^7.2.0",
        "path-to-regexp": "^8.3.0",
        "primeflex": "^4.0.0",
        "primeicons": "^7.0.0",
        "primereact": "^10.9.7",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-icons": "^5.5.0",
        "react-router": "^7.9.6",
        "react-router-dom": "^7.9.6",
        "tailwind-merge": "^3.4.0",
        "tailwind-variants": "^3.2.2",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@iconify/icons-mdi": "^1.2.48",
        "@iconify/json": "^2.2.408",
        "@iconify/tailwind4": "^1.1.0",
        "@storybook/addon-docs": "10.0.8",
        "@storybook/addon-onboarding": "10.0.8",
        "@storybook/react-vite": "10.0.8",
        "@tailwindcss/vite": "^4.1.17",
        "@types/lodash": "^4.17.20",
        "@types/react": "^19.2.6",
        "@types/react-dom": "^19.2.3",
        "eslint-plugin-storybook": "10.0.8",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "storybook": "10.0.8",
        "ts-node": "^10.9.2"
    }
}
