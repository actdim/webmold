{
    "name": "webapp",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "packageManager": "pnpm@10.30.3",
    "scripts": {
        "dev": "vite",
        "cache:clear": "shx rm -rf node_modules/.vite",
        "online": "vite --host",
        "preview": "vite preview",
        "test": "npx vitest --config=vitest.node.config.ts --no-cache",
        "test:w": "npx vitest --config=vitest.node.config.ts --watch",
        "test:v8": "npx vite",
        "lint": "eslint . -f visualstudio --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
        "build": "tsc -b && vite build",
        "pnpm:u": "pnpm update",
        "pnpm:ou": "pnpm outdated",
        "ncu:l": "ncu",
        "ncu:u": "ncu -u",
        "nvm:la": "nvm list available",
        "nvm:il": "nvm install lts",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "tw:i": "npx @tailwindcss/cli init",
        "tw:w": "npx @tailwindcss/cli -i ./src/app.css -o ./src/app-full.css --watch",
        "postcss": "ts-node postcss.ts",
        "az-cr:cl": "powershell -ExecutionPolicy Bypass -File az_cr_cleanup.ps1"
    },
    "dependencies": {        
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@iconify/react": "^6.0.2",
        "@mui/material": "^7.3.8",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",                                
        "primeflex": "^4.0.0",
        "primeicons": "^7.0.0",
        "primereact": "^10.9.7",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-icons": "^5.5.0",
        "react-router": "^7.13.1",
        "react-router-dom": "^7.13.1",
        "tailwind-merge": "^3.5.0",
        "tailwind-variants": "^3.2.2"
    },
    "devDependencies": {
        "@iconify/icons-mdi": "^1.2.48",
        "@iconify/json": "^2.2.443",
        "@iconify/tailwind4": "^1.2.1",
        "@storybook/addon-docs": "10.2.13",
        "@storybook/addon-onboarding": "10.2.13",
        "@storybook/react-vite": "10.2.13",
        "@tailwindcss/vite": "^4.2.1",        
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "eslint-plugin-storybook": "10.2.13",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "storybook": "10.2.13",
        "ts-node": "^10.9.2"
    }
}
